{
  "multipart":[
    {
      "apply": {"model":"flamereaction:block/alcohol_lamp"}
    },

    {
      "when": {"fired":"true","o_colored": "true"},
      "apply": {"model": "flamereaction:block/alcohol_lamp_fire_gray"}
    },
    {
      "when": {"fired":"true","o_colored": "false"},
      "apply": {"model": "flamereaction:block/alcohol_lamp_fire"}
    },
    {
      "when": {"fired": "false"},
      "apply": {"model": "flamereaction:block/alcohol_lamp_hat"}
    },

    {
      "when": {"has_iron_stand": "true"},
      "apply": {"model": "flamereaction:block/iron_stand"}
    }
  ]
}